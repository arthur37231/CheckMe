<?xml version="1.0" encoding="utf-8"?>
<androidx.coordinatorlayout.widget.CoordinatorLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id = "@+id/myCodeLayout"
    android:layout_width="match_parent"
    android:layout_height="match_parent">


    <comp5216.sydney.edu.au.checkme.view.TitleBarLayout
        android:id="@+id/myCodeTitle"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"/>

    <ListView
        android:id="@+id/codeListView"
        android:layout_width="match_parent"
        android:layout_height="431dp"
        android:layout_gravity="bottom|end"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.333"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintVertical_bias="0.0" />

    <com.google.android.material.floatingactionbutton.FloatingActionButton
        android:id="@+id/createNewEvent"
        android:layout_width="56dp"
        android:layout_height="56dp"
        android:layout_gravity="bottom|end"
        android:layout_margin="16dp"
        android:backgroundTint="@color/lake_blue"
        app:rippleColor="@color/light_grey"
        app:borderWidth="0dp"
        android:src="@drawable/ic_add_24"
        android:elevation="6dp"
        android:contentDescription="@string/my_code_create_new_event_button" />

</androidx.coordinatorlayout.widget.CoordinatorLayout>